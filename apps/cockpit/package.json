{
  "name": "@econeura/cockpit-dev",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "esbuild src/index.tsx --bundle --format=esm --target=es2020 --outfile=dist/app.js --loader:.tsx=tsx --loader:.ts=ts --jsx=automatic",
    "watch": "npm run build -- --watch",
    "test": "vitest",
    "test:ci": "vitest run --reporter=dot --run",
    "lint": "eslint \"src/**\" --ext .ts,.tsx --max-warnings=0",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test:coverage": "vitest run --coverage",
    "lint:fix": "eslint \"src/**\" --ext .ts,.tsx --max-warnings=0 --fix"
  },
  "dependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "lucide-react": "0.441.0"
  },
  "devDependencies": {
    "esbuild": "0.25.0",
    "vitest": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/jest-dom": "^6.0.0",
    "jsdom": "^27.0.0"
  }
}
